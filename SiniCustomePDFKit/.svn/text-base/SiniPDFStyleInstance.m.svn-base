#import "SiniPDFStyleInstance.h"
@interface SiniPDFStyleInstance ()

@end

@implementation SiniPDFStyleInstance

-(instancetype)init
{
    if (self = [super init]) {
        [self setUp];
    }
    return self;
}
+ (instancetype)defaultInstance
{
    return [[[SiniPDFStyleInstance class] alloc]init];
}
-(void)setUp
{
    self.annotationColor = SiniAnnotionLineColor;
    self.cornerToolbarBackgroundColor = SiniMAINVIEWCOLOR;
    self.annotationLineWith = 2.0f;
    
}




@end
